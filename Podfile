# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'brainshareapp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for brainshareapp
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'

  target 'brainshareappTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'brainshareappUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
